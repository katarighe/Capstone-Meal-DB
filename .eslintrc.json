{
    "env": {
      "browser": true,
      "es6": true,
      "jest": true
    },
    "parser": "babel-eslint",
    "parserOptions": {
      "ecmaVersion": 2018,
      "sourceType": "module"
    },
    "extends": ["airbnb-base"],
    "rules": {
      "no-shadow": "off",
      "linebreak-style": "off",
      "import/no-cycle": "off",
      "no-console": "off",
      "prefer-destructuring": "off",
      "no-param-reassign": "off",
      "no-undef": "off",
      "no-empty": "off",
      "eol-last": "off",
      "import/extensions": [ 1, {
        "js": "always", "json": "always"
      }]
    },
    "ignorePatterns": [
      "dist/",
      "build/"
    ]
  }